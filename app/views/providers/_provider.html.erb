<div id="<%= dom_id provider %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Name:</strong>
    <%= provider.name %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Contact email:</strong>
    <%= provider.contact_email %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Branch name:</strong>
    <%= provider.branch.name %>
  </p>

</div>
